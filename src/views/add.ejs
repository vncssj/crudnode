<html>

<head>
    <title></title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
        integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
</head>

<body>

    <div class="container">
        <div class="jumbotron">
            New User
        </div>
        <form action="/add" method="POST">
            <div class="form-group">
                <input class="form-control" name="nome" placeholder="Insira seu nome" />
            </div>
            <div class="form-group">
                <input class="form-control" name="sobrenome" placeholder="Insira seu sobrenome" />
            </div>
            <div class="form-group">
                <input class="form-control" name="nascimento" placeholder="Insira seu nascimento" />
            </div>
            <div class="form-group">
                <input class="form-control" name="email" placeholder="Insira seu email" />
            </div>
            <div class="form-group">
                <input class="form-control" name="cep" placeholder="Insira seu cep" />
            </div>
            <div class="form-group">
                <input class="form-control" name="logradouro" placeholder="Insira seu logradouro" />
            </div>
            <div class="form-group">
                <input class="form-control" name="cidade" placeholder="Insira seu cidade" />
            </div>
            <div class="form-group">
                <input class="form-control" name="estado" placeholder="Insira seu estado" />
            </div>
            <div class="form-group">
                <input class="form-control" name="pais" placeholder="Insira seu pais" />
            </div>
            <div class="form-group">
                <input class="form-control" name="complemento" placeholder="Insira seu complemento" />
            </div>
            <div class="form-group">
                <input class="form-control" name="numero" placeholder="Insira seu numero" />
            </div>
            <input class="btn btn-primary offset-4 col-4" type="submit" value="Salvar" />
        </form>
    </div>
</body>

</html>